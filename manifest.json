{
  "manifest_version": 3,
  "name": "Focus Timer",
  "version": "1.4",
  "description": "A focus timer with motivational quotes.",
  "permissions": ["storage", "notifications", "alarms"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
